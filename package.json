{
  "name": "MySurreyInvoiceApp",
  "version": "0.0.1",
  "scripts": {
    "start": "node tools/run.js",
    "build": "NODE_ENV=production node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "test:lint": "eslint src test tools postcss.config.js webpack.config.js",
    "test:unit": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofsurrey/mysurrey-app.git"
  },
  "author": "City of Surrey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cityofsurrey/mysurrey/issues"
  },
  "dependencies": {
    "apollo-client": "0.8.7",
    "babel-polyfill": "6.22.0",
    "fastclick": "1.0.6",
    "graphql-tag": "1.2.4",
    "immutability-helper": "2.1.2",
    "moment": "2.17.1",
    "radium": "^0.18.2",
    "react": "15.4.2",
    "react-apollo": "0.11.1",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "5.0.2",
    "react-router-dom": "4.0.0",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.0",
    "autoprefixer": "6.6.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "babel-runtime": "6.22.0",
    "browser-sync": "2.18.6",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.26.1",
    "ejs": "2.5.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "file-loader": "0.9.0",
    "front-matter": "2.1.1",
    "json-loader": "0.5.4",
    "path-to-regexp": "1.7.0",
    "pixrem": "3.0.2",
    "pleeease-filters": "3.0.0",
    "postcss": "5.2.10",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-custom-selectors": "3.0.0",
    "postcss-flexbugs-fixes": "2.1.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.2.2",
    "postcss-media-minmax": "2.1.2",
    "postcss-nesting": "2.3.1",
    "postcss-selector-matches": "2.0.5",
    "postcss-selector-not": "2.0.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webp-loader": "0.1.1",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "react/jsx-filename-extension": "off",
      "react/require-default-props": "off"
    },
    "settings": {
      "import/resolver": "webpack"
    }
  }
}
